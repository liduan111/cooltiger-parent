<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kyj.cooltiger.cooltigerproduct.mapper.ProductSpecValueMapper">

    <!-- 添加商品规格值 -->
    <insert id="addProductSpecValue" parameterType="ProductSpecValue" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO ky_product_spec_value
            (value, spec_name_id, order)
        VALUES
            (#{productSpecValue.value},#{productSpecValue.specNameId},#{productSpecValue.order})
    </insert>
</mapper>